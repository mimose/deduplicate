        <deduplicate-core.version>1.0-SNAPSHOT</deduplicate-core.version>

    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.mimose.component</groupId>
                <artifactId>deduplicate-all</artifactId>
                <version>${deduplicate-all.version}</version>
