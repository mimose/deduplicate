                <version>${deduplicate-cache-map.version}</version>
            </dependency>

            <dependency>
                <groupId>com.mimose.component</groupId>
                <artifactId>deduplicate-gen-api</artifactId>
                <version>${deduplicate-gen-api.version}</version>
            </dependency>
            <dependency>
                <groupId>com.mimose.component</groupId>
                <artifactId>deduplicate-gen-jackson</artifactId>
                <version>${deduplicate-gen-jackson.version}</version>
            </dependency>

        </dependencies>
